{
  "name": "vexilla/client",
  "description": "A client for the Vexilla Feature Flag System",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Chris Griffing",
      "email": "cmgriffing@gmail.com"
    }
  ],
  "require": {
    "ext-mbstring": "*",
    "nesbot/carbon": "^2.71.0",
    "pcov/clobber": "^2.0"
  },
  "require-dev": {
    "php": "^8.1",
    "phpunit/phpunit": "^9.3.5"
  },
  "autoload": {
    "psr-4": {
      "Vexilla\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vexilla\\": "src/",
      "VexillaTest\\": "tests/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit --coverage-text --verbose"
  }
}
